<?xml version="1.0"?>
<config version="7.1.0" urldb="paloaltonetworks">
  <mgt-config>
    <users>
      <entry name="admin">
        <phash>***************</phash>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
      ...
    </users>
  </mgt-config>
  <shared>
  ...
  </shared> 
  <devices>
    <entry name="localhost.localdomain">
      <network>
        <interface>
          <ethernet>
            <entry name="ethernet1/1">
              <layer3>
                <ipv6>
                  <neighbor-discovery>
                    <router-advertisement>
                      <enable>no</enable>
                    </router-advertisement>
                  </neighbor-discovery>
                </ipv6>
                <ndp-proxy>
                  <enabled>no</enabled>
                </ndp-proxy>
                <lldp>
                  <enable>no</enable>
                </lldp>
                <units>
                  <entry name="ethernet1/1.99">
                    <ipv6>
                      <neighbor-discovery>
                        <router-advertisement>
                          <enable>no</enable>
                        </router-advertisement>
                      </neighbor-discovery>
                    </ipv6>
                    <ndp-proxy>
                      <enabled>no</enabled>
                    </ndp-proxy>
                    <ip>
                      <entry name="10.20.80.1/25"/>
                    </ip>
                    <interface-management-profile>allow_mgmt_from_regioit</interface-management-profile>
                    <tag>99</tag>
                  </entry>
                  ...
                </units>
                <netflow-profile>regioit-netflow</netflow-profile>
              </layer3>
            </entry>
            <entry name="ethernet1/8">
              <layer3>
                <ipv6>
                  <neighbor-discovery>
                    <router-advertisement>
                      <enable>no</enable>
                    </router-advertisement>
                  </neighbor-discovery>
                </ipv6>
                <ndp-proxy>
                  <enabled>no</enabled>
                </ndp-proxy>
                <lldp>
                  <enable>no</enable>
                </lldp>
                <interface-management-profile>allow_mgmt_from_internet</interface-management-profile>
                <ip>
                  <entry name="public IP"/>
                </ip>
                <netflow-profile>regioit-netflow</netflow-profile>
              </layer3>
            </entry>
          </ethernet>
          <loopback/>
          <vlan>
            <ipv6>
              <neighbor-discovery>
                <router-advertisement>
                  <enable>no</enable>
                </router-advertisement>
              </neighbor-discovery>
            </ipv6>
            <ndp-proxy>
              <enabled>no</enabled>
            </ndp-proxy>
            <units/>
          </vlan>
          <tunnel>
            <units>
              <entry name="tunnel.1">
                <interface-management-profile>allow_mgmt_from_regioit</interface-management-profile>
                <comment>Management-Netze</comment>
                <ip>
                  <entry name="Tunnel IP"/>
                </ip>
              </entry>
              ...
            </units>
          </tunnel>
        </interface>
        <vlan/>
        <virtual-wire/>
		...
        <ike>
          <crypto-profiles>
            <ike-crypto-profiles>
              <entry name="p1_aes256_sha512_dh20_28800">
                <hash>
                  <member>sha512</member>
                  <member>sha512</member>
                </hash>
                <dh-group>
                  <member>group20</member>
                </dh-group>
                <encryption>
                  <member>aes-256-cbc</member>
                </encryption>
                <lifetime>
                  <hours>8</hours>
                </lifetime>
              </entry>
            </ike-crypto-profiles>
            <ipsec-crypto-profiles>
              <entry name="p2_esp_aes256_sha512_dh20_3600">
                <esp>
                  <authentication>
                    <member>sha512</member>
                  </authentication>
                  <encryption>
                    <member>aes-256-cbc</member>
                  </encryption>
                </esp>
                <lifetime>
                  <hours>1</hours>
                </lifetime>
                <dh-group>group20</dh-group>
              </entry>
            </ipsec-crypto-profiles>
          </crypto-profiles>
          <gateway>
            <entry name="regioit_mgmt--AKBK">
              <authentication>
                <pre-shared-key>
                  <key>*******************************</key>
                </pre-shared-key>
              </authentication>
              <protocol>
                <ikev1>
                  <dpd>
                    <enable>yes</enable>
                  </dpd>
                  <ike-crypto-profile>p1_aes256_sha512_dh20_28800</ike-crypto-profile>
                  <exchange-mode>main</exchange-mode>
                </ikev1>
                <ikev2>
                  <dpd>
                    <enable>yes</enable>
                  </dpd>
                </ikev2>
              </protocol>
              <protocol-common>
                <nat-traversal>
                  <enable>no</enable>
                </nat-traversal>
                <fragmentation>
                  <enable>no</enable>
                </fragmentation>
              </protocol-common>
              <local-address>
                <interface>ethernet1/8</interface>
                <ip>Local Public IP</ip>
              </local-address>
              <peer-address>
                <ip>Peer Public IP</ip>
              </peer-address>
              <disabled>no</disabled>
            </entry>
          </gateway>
        </ike>
        <tunnel>
          <ipsec>
            <entry name="regioit_mgmt--AKBK">
              <auto-key>
                <ike-gateway>
                  <entry name="regioit_mgmt--AKBK"/>
                </ike-gateway>
                <ipsec-crypto-profile>p2_esp_aes256_sha512_dh20_3600</ipsec-crypto-profile>
              </auto-key>
              <tunnel-monitor>
                <enable>no</enable>
              </tunnel-monitor>
              <tunnel-interface>tunnel.1</tunnel-interface>
              <disabled>no</disabled>
            </entry>
          </ipsec>
        </tunnel>
      </network>
      <vsys>
        <entry name="vsys1">
          <application/>
          <application-group/>
          <zone>
            <entry name="mgmt">
              <network>
                <layer3>
                  <member>ethernet1/1.99</member>
                </layer3>
              </network>
            </entry>
            ...
            <entry name="vpn">
              <network>
                <layer3>
                  <member>tunnel.1</member>
                </layer3>
              </network>
            </entry>
          </zone>
          <rulebase>
            <security>
              <rules>
                ...
                <entry name="rule.mgmt.in.test">
                  <to>
                    <member>mgmt</member>
                  </to>
                  <from>
                    <member>vpn</member>
                  </from>
                  <source>
                    <member>10.104.136.200/32</member>
                  </source>
                  <destination>
                    <member>10.20.80.10/32</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>any</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                  <rule-type>interzone</rule-type>
                  <profile-setting>
                    <group>
                      <member>regioit</member>
                    </group>
                  </profile-setting>
                  <tag>
                    <member>Daniel</member>
                    <member>Test Rule</member>
                    <member>Project</member>
                  </tag>
                  <description>Final Project Test Rule... (13.02.2017)</description>
                </entry>
                </rules>
            </security>
            <default-security-rules>
              <rules>
                <entry name="intrazone-default">
                  <action>deny</action>
                  <log-start>no</log-start>
                  <log-end>yes</log-end>
                </entry>
              </rules>
            </default-security-rules>
          </rulebase>
          <ssl-tls-service-profile/>
          <tag>
		     ...
            <entry name="Daniel"/>
            <entry name="Test Rule"/>
            <entry name="Project"/>
          </tag>
        </entry>
      </vsys>
    </entry>
  </devices>
</config>
